<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ProjectDubai.Styles">
    <Style x:Key="rsButtonToolbar" TargetType="Button">
        <Setter Property="FontFamily" Value="Segoe MDL2 Assets"></Setter>
        <Setter Property="Width" Value="44"></Setter>
        <Setter Property="Background" Value="#10bbb3"></Setter>
        <Setter Property="Height" Value="44"></Setter>
        <Setter Property="Foreground" Value="#F2F2F2"></Setter>
    </Style>
</ResourceDictionary>
